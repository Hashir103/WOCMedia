		<USERNAME_MIN_LENGTH>4</USERNAME_MIN_LENGTH>
		<USERNAME_MAX_LENGTH>64</USERNAME_MAX_LENGTH>
		
		<DNAME_MIN_LENGTH>4</DNAME_MIN_LENGTH>
		<DNAME_MAX_LENGTH>16</DNAME_MAX_LENGTH>
		
		<PASSWORD_MIN_LENGTH>4</PASSWORD_MIN_LENGTH>
		<PASSWORD_MAX_LENGTH>25</PASSWORD_MAX_LENGTH>
		
		<EMAIL_MAX_LENGTH>150</EMAIL_MAX_LENGTH>
		
		<NAME_MIN_LENGTH>1</NAME_MIN_LENGTH>
