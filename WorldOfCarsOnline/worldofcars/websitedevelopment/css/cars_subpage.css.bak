/*-------------- Manage Account Page ---------------*/
#manage_account #content,#cancel_sponsorship #content, #purchase #content{
	float:left;
	margin-top:10px;
}
#manage_account .col_left,#cancel_sponsorship .col_left, #purchase .col_left{
	float:left;
	width:364px;
	padding:0 10px 0 20px;
	color:#1c1c1c;
}
#manage_account .col_left p{
	margin-bottom:5px;
}
#manage_account .col_left ul{
	margin-bottom:20px;
}
#manage_account .col_left li{
	padding-left:5px;
	line-height:1.3em;
}
#manage_account .col_right,#cancel_sponsorship .col_right,#purchase .col_right{
	float:left;
	width:500px;
}
#manage_account .col_right h3{
	font-size:16px;
}
.account_mod,#cancel_sponsorship .cancel_sponsorship_mod{
	float:left;
	position:relative;
	width:464px;
	height:241px;
	padding:10px 20px 10px 15px;
	margin-bottom:10px;
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/backgrounds/mod_499x261.png) left top no-repeat;
}
.account_mod h3{
	float:left;
	width:280px;
}
.account_mod .flush_right,.forgot_login{
	float:right;
	color:#459BC9;
	font-size:11px;
	padding-top:3px;
	text-decoration:underline;
}
.col_right .gray_divider{
	float:left;
	width:464px;
	height:1px;
	overflow:hidden;
	margin:4px 0;
	background: #797979;
}
.account_mod fieldset,.cancel_sponsorship_mod fieldset{
	clear:both;
}
.account_mod label,.cancel_sponsorship_mod label{
	float:left;
	font-weight:bold;
	width:160px;
	text-align:right;
	padding:3px 10px 0 0;
}
.forgot_login{
	margin:10px 20px 10px 0;
}
.account_mod input,.cancel_sponsorship_mod input{
	float:left;
	position:relative;
	display:block;
	width:258px;
	height:25px;
	padding:5px 10px 0 10px;
	margin-bottom:20px;
	border:none;
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/backgrounds/input_bg_278x31.gif) left top no-repeat;
}
input.required{
	color:#000;
}
.account_mod .login_btn,.cancel_sponsorship_mod .login_btn{
	clear:both;
	margin:0em auto;
}
.cancel_sponsorship_mod{
	float:left;
	width:468px;
	height:105px;
	padding:10px 15px 10px 15px;
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/backgrounds/mod_498x125.png) left top no-repeat;
}
.cancel_sponsorship_mod p{
	float:left;
	width:220px;
}
.cancel_sponsorship{
	float:right;
	display:block;
	width:234px;
	height:34px;
	margin-top:15px;
	overflow:hidden;
	text-indent: -9999em;
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/buttons/cancel-sponsorship-36.gif) left top no-repeat;
}
.cancel_sponsorship:hover{
	background-position: 0 -36px;
}
#cancel_sponsorship .error_page .col_right,#cancel_sponsorship .confirmation .col_right{
	position:absolute;
	right:0;
	top:80px;
	width:485px;
}
#cancel_sponsorship .error_page .col_right h3{
	font-size:16px;
}
.error_page .col_right p,.confirmation .col_right p{
	margin-top:50px;
	clear:left;
	float:left;
	width:455px;
}
#cancel_sponsorship .error_page .cancel_sponsorship{
	float:none!important;
	margin:15px auto 0;
}
#page_content.confirmation .col_right p,#page_content.confirmation .col_right ul{
	margin:0 0 25px!important;
}
