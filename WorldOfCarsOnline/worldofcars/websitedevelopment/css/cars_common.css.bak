#nav .nav_racecode a{
	width:139px;
	background-position: -281px 0px;
}
#nav .nav_racecode a:hover,.sec_racecode #nav .nav_racecode a{
	width:139px;
	background-position: -281px -102px;
}
.sec_about #nav .nav_racecode a:hover{
	background-position: -281px -102px;
}
.sec_help #nav .nav_racecode a:hover{
	width:133px;
	margin-right:-14px;
	background-position: -281px -102px;
	z-index:500;
}
#nav .nav_help a{
	width:141px;
	background-position: -420px 0;
}
#nav .nav_help a:hover,.sec_help #nav .nav_help a{
	width:141px;
	background-position: -420px -136px;
}
.sec_parents #nav .nav_help a:hover{
	background-position: -420px -136px;
}
#subnav_bg{
	position:absolute;
	width:496px;
	height:36px;
	padding:15px 10px 0;
	left:240px;
	top:50px;
	background:url(http://a.dolimg.com/worldofcars/websitedevelopment/images/backgrounds/subnav_bg.png) left top no-repeat;
}
.sec_community #subnav_bg{
	width:527px;
	left:230px;
	background:url(http://a.dolimg.com/worldofcars/websitedevelopment/images/backgrounds/subnav_bg_comm.png) left top no-repeat;
}
#about_game #subnav_bg{
	display:none;
}
.subnav{
	display:none;
	float:left;
	margin-top:-4px;
	padding:5px 0 0 60px;
}
#community_sub{
	padding-left:35px;
}
#parents_sub{
	padding-left:190px;
}
#help_sub,#sponsorship_sub{
	padding-left:65px;
}
.sec_community #community_sub,.sec_sponsorship #sponsorship_sub,.sec_parents #parents_sub,.sec_help #help_sub{/*.sec_about #sponsorship_sub,*/
	display:block;
}
.subnav li{
	float:left;
	padding: 0 15px;
	line-height:1em;
	border-right:2px solid #767676;
}
#community_sub li{
	padding:0 28px;
}
@media screen and (-webkit-min-device-pixel-ratio:0){
	#community_sub li{
		padding:0 5px;
	}
}
#help_sub li{
	padding: 0 30px;
}
.subnav li a{
	position:relative;
	color:#767676;
	font-weight:bold;
}
.subnav li a:hover,.subnav .selected,#purchase .sub_sponsorship{
	color:#459bc9;
}

/* end navigation */

.header_play{
	float:left;
	display:block;
	width:200px;
	height:84px;
	margin:-20px 0 0 10px;	
	position:relative;
	background: url(../images/buttons/play_up.png) 10px 26px no-repeat;
}

.header_create-a-car{
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/buttons/create_a_car.png) no-repeat;
	display:block;
	height:57px;
	margin-left:10px;
	margin-top:26px;
	overflow:hidden;
	width:174px;
}
.header_create-a-car:hover{
	background-position:0px -57px;
}
/*-- end header --*/

#content{
	float:left;
	padding:10px 0 0 10px;
}

/*---------------- util ----------------*/
.form_el{
	float:left;
	display:block;
	width:auto;
	position:relative;
}
.error_msg{
	position:absolute;
	color:#ff0000;
	left:0;
	/*top:30px;*/
}

/*---------- footer ----------*/

#footer{
	clear:both;
	float:left;
	width:100%;
	padding-top:10px;
}
.footlinks{
	width:100%;
	text-align:center;
}
.footlinks li{
	display:inline;
}
#footer,#footer td,#footer td span{
	color:#555!important;	
}
#footer a{
	color:#555!important;
	text-decoration:none;
}
#footer hr{
	border-top-color:#555!important;
}

#blk_alpha{
	display:none;
	position:absolute;
	height:100%;
	width:100%;
	top:0;
	left:0;
	background: #000;
	z-index:999998;
	filter:alpha(opacity=70);
	-moz-opacity:0.7;
	-khtml-opacity: 0.7;
	opacity: 0.7;
}

/*---- resusables ------*/

.clear{clear:both;}
.lt_blue{color:#b9cddd;}
.cyan{color:#53aad7;}
.required{color:#ff0000;}
.noborderplease{border:none!important;}
.cars_orange{color:#ef5319;font-weight:bold;}
.nomargin{margin:0px!important;}
.hideme{display:none;!important}