	float:left;
	display:block;
	background: url(http://a.dolimg.com/worldofcars/websitedevelopment/images/buttons/woc_nav.gif) top left no-repeat;
	text-indent:-9999em;
	overflow:hidden;
	height:34px;
}
#nav .nav_community a{
	width:138px;
	background-position: 0 0;
}
#nav .nav_community a:hover,.sec_community #nav .nav_community a{
	background-position: 0 -34px;
}
.sec_sponsorship #nav .nav_community a:hover{
	width:138px;
	margin-right:-9px;
	background-position: 0 -34px;
	z-index:500;
}
