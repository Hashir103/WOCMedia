<?xml version="1.0" encoding="utf-8"?>
<data>
	
	<!-- =========================== 
		 PRELOAD ASSETS 
	===========================-->
	<LoaderMax name="preloader_assets_queue">
		<SWFLoader name="preloader" url="{assetsURL}/shared/swf/preload_assets.swf" context="own" />
		<ImageLoader name="preloaderImage" centerRegistration="true" url="{assetsURL}/localised/{locale}/images/cars_logo.png" />
		<ImageLoader name="introImage" centerRegistration="false" url="{assetsURL}/localised/{locale}/images/intro_fin.png" />
		<ImageLoader name="ratingImage" centerRegistration="true" url="{assetsURL}/localised/{locale}/images/rating.png" />
	</LoaderMax>
	
	<LoaderMax name="main_assets_queue">
		<SWFLoader url="{assetsURL}/shared/swf/main_assets.swf" context="own" />
	</LoaderMax>
	
	
	<!-- =========================== 
		 Trailer
	=========================== -->
	<trailer localise_id="trailer">
		<link id="trailer.skin">{assetsURL}/shared/swf/SkinOverPlaySeekFullVol.swf</link>
		<link id="trailer.trailerURL">{assetsURL}/localised/{locale}/video/trailer.flv</link>
		<copy copy="trailer.seeInCinemas" stage_instance="seeInCinemas" />
	</trailer>
	
	<!-- =========================== 
		 COPY
	===========================-->
	<frame localise_id="frame">
		<image image="{assetsURL}/localised/{locale}/images/cars_logo.png" stage_instance="titleTreatment" centerRegistration="true" />
		<image image="{assetsURL}/localised/{locale}/images/rating.png" stage_instance="frameRating" centerRegistration="true" />
		<copy copy="frame.seeInCinemas" stage_instance="cinemaCopy" />
		<copy copy="frame.visitSite" stage_instance="visitSiteButton" />
		<copy copy="frame.watchTrailer" stage_instance="viewTrailerButton" />
		<copy copy="frame.becomeFan" stage_instance="facebookButton.tf1" />
		<copy copy="frame.onFacebook" stage_instance="facebookButton.tf2" />
		<copy copy="frame.shareThis" stage_instance="shareCopy" />
		<copy copy="frame.copyrightCopy" stage_instance="copyrightCopy" />
		
		<!-- Remove the terms and privacy links to prevent them from being displayed -->
		<link id="frame.termsLink">http://corporate.disney.go.com/corporate/terms.html</link>
		<link id="frame.privacyLink">http://corporate.disney.go.com/corporate/pp_wdig.html</link>
		
		<link id="frame.visitSiteLink">http://www.facebook.com/DisneyPixar#!/PixarCars</link>
		<link id="frame.fbFollowLink">http://www.facebook.com/DisneyPixar#!/PixarCars</link>
		<link id="frame.siteURL">http://www.cars2megaposter.com/</link>
		<link id="frame.facebookShareImageURL">{assetsURL}/shared/images/cars_logo.jpg</link>
		<link id="frame.imagination">http://www.imagination.at</link>
		<button copy="frame.take" stage_instance="btnTakeTheQuiz" />
		<button copy="frame.found" stage_instance="found" />
	</frame>
	
	<quiz localise_id="quiz">
		<button copy="quiz.start" stage_instance="btn_start" />
		<button copy="quiz.no" stage_instance="btn_no" />
		<button copy="quiz.yes" stage_instance="btn_yes" />
		<copy copy="quiz.intro" stage_instance="txt_intro" />
		<copy copy="quiz.closeTxt" stage_instance="closeTxt" />
	</quiz>
	
	<intro localise_id="intro">
		<image image="{assetsURL}/localised/{locale}/images/intro_fin.png" stage_instance="fin" centerRegistration="false"/>
		<button copy="intro.go" stage_instance="go" />
		<copy copy="intro.hd" stage_instance="hd" />
	</intro>
	
	
	<popup localise_id="popup">
		<copy copy="popup.fact" stage_instance="btnFacts" />
		<copy copy="popup.facebook" stage_instance="btnFacebook" />
	</popup>

	
